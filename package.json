{
  "name": "athena-contracts",
  "description": "Athena Cover - Smart Contracts",
  "author": "blackwhale.eth",
  "version": "0.3.0",
  "license": "UNLICENSED",
  "main": "hardhat.config.ts",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test ./test/index.ts",
    "typechain": "typechain --target ethers-v6 --out-dir typechain",
    "deploy-hardhat": "npx hardhat run --network hardhat scripts/deploy/deployAthena.ts",
    "deploy-goerli": "npx hardhat run --network goerli scripts/deploy/deployAthena.ts",
    "make-pool-goerli": "npx hardhat run --network goerli scripts/deploy/deployPool.ts",
    "gen-exports": "npx ts-node scripts/gen/gen-exports.ts",
    "gen-fnsigs": "npx ts-node scripts/gen/gen-fnsigs.ts",
    "gen-interfaces": "npx ts-node scripts/gen/gen-interfaces.ts && npx prettier --write contracts/interfaces"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@openzeppelin/contracts": "^5.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/async": "^3.2.23",
    "@types/chai": "^4.3.8",
    "@types/chai-as-promised": "^7.1.6",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.4",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "async": "^3.2.5",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.8.1",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "prettier": "^3.0.3",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "overrides": {
    "@typechain/ethers-v6": "^0.5.1"
  }
}
